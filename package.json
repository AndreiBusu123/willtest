{
  "name": "therapeutic-ai-system",
  "version": "1.0.0",
  "description": "AI-powered sentiment analysis and therapeutic conversation system",
  "main": "src/backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd src/backend && npm run dev",
    "dev:frontend": "cd src/frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd src/frontend && npm run build",
    "build:backend": "cd src/backend && npm run build",
    "start": "cd src/backend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd src/backend && npm test",
    "test:frontend": "cd src/frontend && npm test",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "deploy": "bash scripts/deploy.sh"
  },
  "keywords": [
    "ai",
    "therapy",
    "sentiment-analysis",
    "mental-health",
    "conversation",
    "nlp"
  ],
  "author": "Andrei Busuioan",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "docker-compose": "^0.24.2"
  },
  "workspaces": [
    "src/backend",
    "src/frontend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}